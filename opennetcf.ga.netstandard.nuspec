<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>com.opennetcf.analytics.google.pcl</id>
        <version>1.0.17184.0</version>
        <title>OpenNETCF's Google Analytics for Xamarin Forms</title>
        <authors>Chris Tacke</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A simple cross-platform Google Analytics library designed for applications built using Xamarin Forms.</description>

    <dependencies>
      <group>
        <dependency id="Xamarin.Forms" version="2.3" />
      </group>
    </dependencies>
    </metadata>
  <files>
    <!-- Portables -->
    <file src=".\bin\Release\netstandard1.3\OpenNETCF.Google.Analytics.dll" target="lib\netstandard1.3" />

    <!-- Android -->
    <file src=".\bin\Release\netstandard1.3\OpenNETCF.Google.Analytics.dll" target="lib\MonoAndroid10" />
    <file src=".\bin\Release\Android\OpenNETCF.Google.Analytics.Android.dll" target="lib\MonoAndroid10" />

    <!-- iOS -->
    <file src=".\bin\Release\netstandard1.3\OpenNETCF.Google.Analytics.dll" target="lib\Xamarin.iOS10" />
    <file src=".\bin\Release\iOS\OpenNETCF.Google.Analytics.iOS.dll" target="lib\Xamarin.iOS10" />
  </files>
</package>
