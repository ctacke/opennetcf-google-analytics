<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>com.opennetcf.analytics.google.pcl</id>
        <version>1.0.16312.0</version>
        <title>OpenNETCF's Google Analytics for Xamarin Forms</title>
        <authors>Chris Tacke</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A simple cross-platform Google Analytics library designed for applications built using Xamarin Forms.</description>

    <dependencies>
      <group>
        <dependency id="Xamarin.Forms" version="2.3" />
      </group>
    </dependencies>
    </metadata>
  <files>
    <!-- Portables -->
    <file src=".\bin\PCL\Release\OpenNETCF.Google.Analytics.dll" target="lib\portable-net45+netcore45+wpa81+wp8" />
    <file src=".\bin\PCL\Release\OpenNETCF.Google.Analytics.pdb" target="lib\portable-net45+netcore45+wpa81+wp8" />

    <!-- Android -->
    <file src=".\bin\PCL\Release\OpenNETCF.Google.Analytics.dll" target="lib\monoandroid+monotouch" />
    <file src=".\bin\PCL\Release\OpenNETCF.Google.Analytics.pdb" target="lib\monoandroid+monotouch" />
    <file src=".\bin\Android\Release\OpenNETCF.Google.Analytics.Android.dll" target="lib\monoandroid+monotouch" />
    <file src=".\bin\Android\Release\OpenNETCF.Google.Analytics.Android.pdb" target="lib\monoandroid+monotouch" />

    <!-- iOS -->
    <file src=".\bin\PCL\Release\OpenNETCF.Google.Analytics.dll" target="lib\Xamarin.iOS10" />
    <file src=".\bin\PCL\Release\OpenNETCF.Google.Analytics.pdb" target="lib\Xamarin.iOS10" />
    <file src=".\bin\iOS\Release\OpenNETCF.Google.Analytics.iOS.dll" target="lib\Xamarin.iOS10" />
    <file src=".\bin\iOS\Release\OpenNETCF.Google.Analytics.iOS.pdb" target="lib\Xamarin.iOS10" />
  </files>
</package>
